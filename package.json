		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "2.36.0-next.3",
		"phosphor-svelte": "^1.3.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
